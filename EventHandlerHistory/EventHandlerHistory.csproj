<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>KzLibraries.EventHandlerHistory</RootNamespace>
    <TargetFramework>netstandard1.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>

    <Description>EventHandler's event history wrapper</Description>

    <PackageId>EventHandlerHistory</PackageId>
    <Version>0.0.1</Version>
    <Authors>naminodarie</Authors>
    <Company>naminodarie</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/naminodarie/KzLibraries</PackageProjectUrl>
    <RepositoryUrl>https://github.com/naminodarie/KzLibraries</RepositoryUrl>
  </PropertyGroup>

</Project>
